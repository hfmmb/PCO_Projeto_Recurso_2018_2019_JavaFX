<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.indicador.IndicadorController">
  <columnConstraints>
    <ColumnConstraints maxWidth="0.0" minWidth="0.0" prefWidth="0.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="10.0" prefWidth="50.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
      <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="200.0" text="Criar Indicador" GridPane.columnIndex="1">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
         <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Categoria:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label alignment="CENTER_RIGHT" text="Nome:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="Extensoes:" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         <Label text="Unidades:" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         <Button fx:id="buttonDialogIndicadorCancel" onAction="#buttonDialogIndicadorCancelActionListener" text="Cancelar" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="6" />
      <Button fx:id="buttonDialogIndicadorCriar" onAction="#buttonDialogIndicadorCriarActionListener" text="Criar" GridPane.columnIndex="3" GridPane.rowIndex="6" />
      <ComboBox fx:id="comboBoxDialogIndicadorCategoria" onAction="#comboBoxDialogIndicadorCategoriaActionListener" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <ComboBox fx:id="comboBoxDialogIndicadorExtensoes" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="4" />
      <ComboBox fx:id="comboBoxDialogIndicadorUnidades" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="5" />
      <RadioButton fx:id="radioButtonDialogIndicadorManual" onAction="#radioButtonDialogIndicadorManualActionListener" mnemonicParsing="false" text="Manual" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <RadioButton fx:id="radioButtonDialogIndicadorAutomatico" onAction="#radioButtonDialogIndicadorAutomaticoActionListener" text="Automatico" GridPane.columnIndex="3" GridPane.rowIndex="3" />
      <TextField fx:id="textFieldDialogIndicadorNome" GridPane.columnIndex="2" GridPane.rowIndex="2" />
   </GridPane>
